	}
	public Date getFollowTime() {
		return followTime;
	}
	public void setFollowTime(Date followTime) {
		this.followTime = followTime;
	}
	public Integer getFollowType() {
		return followType;
	}
	public void setFollowType(Integer followType) {
		this.followType = followType;
	}
